{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react-jsx"
  },
  "outDir": "../../build/@main/frontend",
  "tsBuildInfoFile": "../../build/@main/frontend.tsbuildinfo",
  "rootDir": "src",
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "../components"
    },
    {
      "path": "../utils"
    }
  ]
}
